{
    "Permission everyone": {
        "prefix": "permissionEveryone",
        "body": "e"
    },
    "Permission broadcaster": {
        "prefix": "permissionBroadcaster",
        "body": "b"
    },
    "Permission subscriber": {
        "prefix": "permissionSubscriber",
        "body": "s"
    },
    "Permission vip": {
        "prefix": "permissionVip",
        "body": "v"
    },
    "Permission moderator": {
        "prefix": "permissionModerator",
        "body": "m"
    },
    "Permission user": {
        "prefix": "permissionUser",
        "body": "u ${1:<username>}"
    },
    "Variable load": {
        "prefix": "varLoad",
        "body": [
            "Variable Load ${1:<name>}",
            "$0"
        ]
    },
    "Variable remove": {
        "prefix": "varRemove",
        "body": [
            "Variable Remove ${1:<name>}",
            "$0"
        ]
    },
    "Variable set": {
        "prefix": "varSet",
        "body": [
            "Variable Set ${1:<name>} ${2:<value>}",
            "$0"
        ]
    },
    "Variable global load": {
        "prefix": "globalLoad",
        "body": [
            "Variable Global Load ${1:<name>}",
            "$0"
        ]
    },
    "Variable global remove": {
        "prefix": "globalRemove",
        "body": [
            "Variable Global Remove ${1:<name>}",
            "$0"
        ]
    },
    "Variable global set": {
        "prefix": "globalSet",
        "body": [
            "Variable Global Set ${1:<name>} ${2:<value>}",
            "$0"
        ]
    },
    "If": {
        "prefix": "if",
        "body": [
            "if ${1:<linesToSkip>} ${2:<value1>} ${3:<comparator>} ${4:<value2>}",
            "$0"
        ]
    },
    "Chat send": {
        "prefix": "chat",
        "body": [
            "Chat Send \"${1:<message>}\"",
            "$0"
        ]
    },
    "Chat whisper": {
        "prefix": "whisper",
        "body": [
            "Chat Whisper ${1:<username>} \"${2:<message>}\"",
            "$0"
        ]
    },
    "On command": {
        "prefix": "onCommand",
        "body": [
            "OnCommand ${1:<permission>} ${2:<cooldown>} !${3:<command>}",
            "$0"
        ]
    },
    "On channel point": {
        "prefix": "onChannelPoint",
        "body": [
            "OnChannelPoint ${1:<rewardName>}",
            "$0"
        ]
    }
}