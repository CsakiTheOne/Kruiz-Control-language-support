{
  "name": "kruiz-control-language-support",
  "displayName": "Kruiz-Control-language-support",
  "author": "CsakiTheOne",
  "publisher": "CsakiTheOne",
  "description": "",
  "repository": "https://github.com/CsakiTheOne/Kruiz-Control-language-support",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "buildDistributable": "vsce package"
  },
  "contributes": {
    "languages": [
      {
        "id": "kruizcontrol",
        "aliases": [
          "Kruiz-Control",
          "kruizcontrol"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kruizcontrol",
        "scopeName": "source.txt",
        "path": "./syntaxes/kruizcontrol.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.javascript": "javascript"
        }
      }
    ],
    "snippets": [
      {
        "language": "kruizcontrol",
        "path": "./snippets/kruizcontrol.json"
      }
    ]
  },
  "dependencies": {
    "@types/vscode": "^1.73.0",
    "node-fetch": "^2.6.9"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.9.4"
  }
}
